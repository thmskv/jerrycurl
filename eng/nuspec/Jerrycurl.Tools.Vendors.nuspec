<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>$PackageId$</id>
        <title>$PackageId$</title>
        <version>$PackageVersion$</version>
        <authors>$PackageAuthors$</authors>
        <owners>$PackageOwners$</owners>
        <copyright>$PackageCopyright$</copyright>
        <license type="expression">$PackageLicense$</license>
        <icon>$PackageIconFile$</icon>
        <iconUrl>$PackageIconUrl$</iconUrl>
        <projectUrl>$PackageProjectUrl$</projectUrl>
        <tags>$PackageTags$</tags>
        <repository type="git" url="$RepositoryUrl$" commit="$GitCommitId$" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Vendor support for Jerrycurl CLI.</description>
        <dependencies>
			<group targetFramework="net6.0">
				<dependency id="Jerrycurl.Tools" version="$PackageVersion$" />
				<dependency id="$MySqlPackageId$" version="$MySqlPackageVersion$" />
				<dependency id="$OraclePackageId$" version="$OraclePackageVersion$" />
				<dependency id="$PostgresPackageId$" version="$PostgresPackageVersion$" />
				<dependency id="$SqlitePackageId$" version="$SqlitePackageVersion$" />
				<dependency id="$SqlServerPackageId$" version="$SqlServerPackageVersion$" />
			</group>
        </dependencies>
    </metadata>
    <files>
        <file src="$IconPath$" target="" />
        <file src="$BinPath$**\Jerrycurl.Tools.Vendors.*" target="lib" />
    </files>
</package>