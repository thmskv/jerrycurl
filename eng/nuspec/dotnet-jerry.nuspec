<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata minClientVersion="3.6">
        <id>$PackageId$</id>
        <title>$PackageId$</title>
        <version>$PackageVersion$</version>
        <authors>$PackageAuthors$</authors>
        <owners>$PackageOwners$</owners>
        <copyright>$PackageCopyright$</copyright>
        <license type="expression">$PackageLicense$</license>
        <icon>$PackageIconFile$</icon>
        <iconUrl>$PackageIconUrl$</iconUrl>
        <projectUrl>$PackageProjectUrl$</projectUrl>
        <tags>$PackageTags$</tags>
        <repository type="git" url="$RepositoryUrl$" commit="$GitCommitId$" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Jerrycurl command-line interface.</description>
		<dependencies>
			<group>
				<dependency id="Jerrycurl.CodeAnalysis.Razor" version="$PackageVersion$" />
			</group>
		</dependencies>
        <packageTypes>
            <packageType name="DotnetTool" />
        </packageTypes>
    </metadata>
    <files>
        <file src="$IconPath$" target="" />
        <file src="$SourcePath$DotnetToolSettings.xml" target="tools\$TargetFramework$\any" />
		<file src="$SourcePath$skeleton.jerry" target="tools\$TargetFramework$\any" />
        <file src="$SourcePath$dotnet-jerry-proxy.csproj" target="tools\$TargetFramework$\any" />
		<file src="$SourcePath$Directory.Build.*" target="tools\$TargetFramework$\any" />
		<file src="$SourcePath$DotNetJerryProxyHost.cs" target="tools\$TargetFramework$\any" />
		<file src="$BinPath$**\*" target="tools\$TargetFramework$\any" />
    </files>
</package>