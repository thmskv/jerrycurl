{
  "$schema": "https://raw.githubusercontent.com/AArnott/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "1.1.0-pre.{height}",
  "assemblyVersion": {
    "precision": "major"
  },
  "nuGetPackageVersion": {
    "semVer": 2.0
  },
  "publicReleaseRefSpec": [
    "^refs/tags/v\\d+\\.\\d+\\.\\d+"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  },
  "pathFilters": [
    ":/",
    ":!/LICENSE",
    ":!/README.md",
    ":!/test/unit/README.md",
    ":!/test/integration/README.md"
  ]
}